{
  "background": {
    "scripts": ["src/js/background.js"]
  },
  "content_scripts": [
    {
      "js": ["src/js/content.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "name": "AddUp",
  "short_name": "AddUp",
  "version": "1.0.0",
  "description": "AddUp est un outil qui vous permet d'additionner tous les chiffres que vous voulez, comptes en banque, sites e-commerce ...",
  "icons": {
    "32": "dist/img/logo-32.png",
    "64": "dist/img/logo-64.png",
    "128": "dist/img/logo-128.png",
    "512": "dist/img/logo-512.png"
  },
  "browser_action": {
    "default_icon": "dist/img/icone.png",
    "default_title": "Add some numbers"
  },
  "permissions": [
    "background",
    "activeTab",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "author": "Joffrey Berrier"
}
